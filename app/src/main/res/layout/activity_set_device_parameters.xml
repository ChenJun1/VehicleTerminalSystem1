<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="#0f0f0f"
              android:orientation="vertical">

    <include layout="@layout/layout_toolbar"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginBottom="5dp"
            android:layout_weight="2">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginRight="5dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="@color/color_171717"
                    android:gravity="center_vertical">

                    <TextView
                        style="@style/fram_text"
                        android:text="设置脉冲系数"/>

                    <EditText
                        style="@style/fram_editText"
                        android:editable="false"
                        android:text="--"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="@color/color_1C1C1C"
                    android:gravity="center_vertical">

                    <TextView
                        style="@style/fram_text"
                        android:text="累计里程设置"/>

                    <EditText
                        style="@style/fram_editText"
                        android:editable="false"
                        android:text="--"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="@color/color_171717"
                    android:gravity="center_vertical">

                    <TextView
                        style="@style/fram_text"
                        android:text="疲劳驾驶门限"/>

                    <EditText
                        style="@style/fram_editText"
                        android:editable="false"
                        android:text="--"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="@color/color_1C1C1C"
                    android:gravity="center_vertical">

                    <TextView
                        style="@style/fram_text"
                        android:text="超时速度阀值"/>

                    <EditText
                        style="@style/fram_editText"
                        android:editable="false"
                        android:text="--"/>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="5dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:background="@mipmap/bg_radio">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="20dp"
                        android:text="匹配休眠模式"
                        android:textColor="@color/colorVehicleParametersText2"
                        android:textSize="21sp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="#171717"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="center_vertical">

                        <TextView
                            style="@style/fram_test_text"
                            android:layout_marginLeft="20dp"
                            android:text="休眠等待时间"
                            android:textColor="@color/colorVehicleParametersText2"/>

                        <TextView
                            style="@style/fram_test_text"
                            android:layout_marginLeft="70dp"
                            android:text="6min"
                            android:textColor="@color/color_D3EE1D"/>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1">

                        <TextView
                            style="@style/fram_test_text"
                            android:layout_marginLeft="20dp"
                            android:text="休眠唤醒时间"
                            android:textColor="@color/colorVehicleParametersText2"/>

                        <TextView
                            style="@style/fram_test_text"
                            android:layout_marginLeft="70dp"
                            android:text="6min"
                            android:textColor="@color/color_D3EE1D"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="110dp"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="20dp"
                            android:layout_marginLeft="20dp"
                            android:layout_marginRight="15dp"
                            android:background="@drawable/vehicle_parameters_bg3"
                            android:gravity="center"
                            android:paddingBottom="5dp"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:paddingTop="5dp"
                            android:text="开机休眠"
                            android:textColor="#000000"
                            android:textSize="@dimen/text_size18"/>

                        <TextView
                            android:layout_width="110dp"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="20dp"
                            android:layout_marginLeft="15dp"
                            android:background="@drawable/vehicle_parameters_bg1"
                            android:gravity="center"
                            android:paddingBottom="5dp"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:paddingTop="5dp"
                            android:text="关机休眠"
                            android:textColor="@color/color_D3EE1D"
                            android:textSize="@dimen/text_size18"/>

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginRight="5dp"
                android:layout_weight="1"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:background="@mipmap/bg_radio">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="20dp"
                        android:text="报警提示方式"
                        android:textColor="@color/colorVehicleParametersText2"
                        android:textSize="21sp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="#171717"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="15dp"
                        android:background="@drawable/vehicle_parameters_bg1"
                        android:gravity="center"
                        android:paddingBottom="5dp"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:paddingTop="5dp"
                        android:text="TTS语音"
                        android:textColor="@color/color_D3EE1D"
                        android:textSize="@dimen/text_size18"/>

                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:background="@drawable/vehicle_parameters_bg1"
                        android:gravity="center"
                        android:paddingBottom="5dp"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"
                        android:paddingTop="5dp"
                        android:text="蜂鸣器"
                        android:textColor="@color/color_D3EE1D"
                        android:textSize="@dimen/text_size18"/>
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:layout_marginLeft="5dp"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:background="@mipmap/bg_radio">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="20dp"
                        android:text="CNSS定位模式"
                        android:textColor="@color/colorVehicleParametersText2"
                        android:textSize="21sp"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="#171717"
                    android:gravity="center_vertical">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center">

                        <TextView
                            android:layout_width="110dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/vehicle_parameters_bg1"
                            android:gravity="center"
                            android:paddingBottom="5dp"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:paddingTop="5dp"
                            android:text="混合定位"
                            android:textColor="@color/color_D3EE1D"
                            android:textSize="@dimen/text_size18"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center">

                        <TextView
                            android:layout_width="110dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/vehicle_parameters_bg1"
                            android:gravity="center"
                            android:paddingBottom="5dp"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:paddingTop="5dp"
                            android:text="GPS定位"
                            android:textColor="@color/color_D3EE1D"
                            android:textSize="@dimen/text_size18"/>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center">

                        <TextView
                            android:layout_width="110dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/vehicle_parameters_bg1"
                            android:gravity="center"
                            android:paddingBottom="5dp"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:paddingTop="5dp"
                            android:text="北斗定位"
                            android:textColor="@color/color_D3EE1D"
                            android:textSize="@dimen/text_size18"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>