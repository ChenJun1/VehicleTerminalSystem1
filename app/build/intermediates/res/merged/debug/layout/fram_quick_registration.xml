<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:layout_marginBottom="20dp"
              android:layout_marginLeft="10dp"
              android:layout_marginRight="10dp"
              android:background="#0f0f0f"
              android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@color/color_171717"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical">

            <TextView
                style="@style/fram_text"
                android:text="终端型号："/>

            <EditText
                android:editable="false"
                android:id="@+id/terminalModel_tv"
                style="@style/fram_editText"
                android:text="--"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical">

            <TextView
                style="@style/fram_text"
                android:text="省域ID："/>

            <EditText
                android:id="@+id/provinceId_tv"
                style="@style/fram_editText"
                android:text="--"/>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@color/color_1C1C1C"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical">

            <TextView
                style="@style/fram_text"
                android:text="制造商ID："/>

            <EditText
                android:editable="false"
                android:id="@+id/manufacturerId_tv"
                style="@style/fram_editText"
                android:text="--"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical">

            <TextView
                style="@style/fram_text"
                android:text="市县城ID："/>

            <EditText
                android:id="@+id/cityId_tv"
                style="@style/fram_editText"
                android:text="--"/>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@color/color_171717"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical">

            <TextView
                style="@style/fram_text"
                android:text="终端ID："/>

            <EditText
                android:editable="false"
                android:id="@+id/terminalId_tv"
                style="@style/fram_editText"
                android:text="--"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical">

            <TextView
                style="@style/fram_text"
                android:text="终端手机号："/>

            <EditText
                android:maxLines="1"
                android:digits="0123456789"
                android:maxLength="11"
                android:inputType="phone"
                android:id="@+id/terminalPhone_tv"
                style="@style/fram_editText"
                android:text="--"/>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@color/color_1C1C1C"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical">

            <TextView
                style="@style/fram_text"
                android:text="车牌号码："/>

            <EditText
                android:id="@+id/vehicleNo_tv"
                style="@style/fram_editText"
                android:text="--"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical">

            <TextView
                style="@style/fram_text"
                android:text="车主手机号："/>

            <EditText
                android:maxLength="11"
                android:maxLines="1"
                android:inputType="phone"
                android:digits="1234567890"
                android:id="@+id/ownerPhone_tv"
                style="@style/fram_editText"
                android:text="--"/>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@color/color_171717"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical">

            <TextView
                style="@style/fram_text"
                android:text="车牌颜色："/>

            <EditText
                android:id="@+id/vehicleColor_tv"
                style="@style/fram_editText"
                android:text="--"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical">

            <TextView
                style="@style/fram_text"
                android:text="车主姓名："/>

            <EditText
                android:id="@+id/ownerName"
                style="@style/fram_editText"
                android:text="--"/>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@color/color_1C1C1C"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical">

            <TextView
                style="@style/fram_text"
                android:text="车辆VIN："/>

            <EditText
                android:id="@+id/vin_tv"
                style="@style/fram_editText"
                android:text="--"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical">
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.3"
        android:background="@color/color_171717"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_marginLeft="20dp"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical">

            <TextView
                android:id="@+id/registration_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/vehicle_parameters_bg1"
                android:paddingBottom="5dp"
                android:paddingLeft="30dp"
                android:paddingRight="30dp"
                android:paddingTop="5dp"
                android:text="快速注册"
                android:textColor="@color/color_D3EE1D"
                android:textSize="@dimen/text_size18"/>
        </LinearLayout>

        <LinearLayout
            android:layout_marginRight="20dp"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical|right">

            <TextView
                android:textColor="@color/colorVehicleParametersText"
                android:textSize="@dimen/text_size18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="状态："/>

            <TextView
                android:id="@+id/content_tv"
                android:textSize="@dimen/text_size18"
                android:textColor="@color/colorVehicleParametersText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="--"/>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>